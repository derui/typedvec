if $(file-exists OMakeroot)
   include MyOCaml
   export

.PHONY: test clean doc all

.SUBDIRS: src test

# mark file confined directory path.
INSTALLED = $(BIG_ROOT)/installed

setup.ml: _oasis
	oasis setup

LIB = typedvec

.DEFAULT: all

install:
	CreateInstalled($(LIB), $(glob i, src/*ml) $(glob i, src/*c))
